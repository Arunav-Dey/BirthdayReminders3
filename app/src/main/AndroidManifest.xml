<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="util.ltd.deysoftwares.birthdayreminders">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/unme"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/unme"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".scrn_1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">

        <intent-filter>
            <action android:name="util.ltd.deysoftwares.birthdayreminders.MainActivity" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>

        </activity>

    </application>

</manifest>